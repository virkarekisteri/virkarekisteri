{
  "name": "virkarekisteri",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npm run i18n-generate-types",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "lint+format": "npm run lint && npm run format",
    "preview": "vite preview",
    "backend": "func start -p 7071 --script-root ../backend",
    "swa:frontend": "swa start",
    "swa": "concurrently --kill-others \"npm run backend\" \"swa start --api-devserver-url http://localhost:7071\"",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "i18n-generate-types": "i18next-resources-for-ts interface -i ./public/locales/fi/ -o \"./src/@types/resources.d.ts\" -c \"!!!Don't try to manually edit!!! This file is auto-generated by i18next-resources-for-ts. See the README if confused.\""
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.1.2",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-final-form": "^6.5.9",
    "react-i18next": "^15.0.3",
    "react-redux": "^9.1.2",
    "react-table": "^7.8.0"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.1",
    "@eslint/js": "^9.11.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@types/react-table": "^7.7.20",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "i18next-resources-for-ts": "^1.5.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  }
}
